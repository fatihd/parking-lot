<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tarnet.domain.ZzzDetail" table="ZZZ_DETAIL">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence_name">SEQ_ZZZ_DETAIL</param>
            </generator>
        </id>

        <property name="numara" column="NUMARA" />

        <many-to-one name="master" lazy="false" class="com.tarnet.domain.ZzzMaster"
                     column="MASTER_ID" not-null="true"/>

    </class>
</hibernate-mapping>
