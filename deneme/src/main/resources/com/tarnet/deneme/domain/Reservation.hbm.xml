<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tarnet.deneme.domain.Reservation" table="ZZZ_RESERVATIONS">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence_name">SEQ_ZZZ_RESERVATIONS</param>
            </generator>
        </id>
        <version name="version"/>

        <property name="userId" column="USER_ID"/>
        <property name="spotId" column="SPOT_ID"/>
        <property name="start" column="START"/>
        <property name="end" column="END"/>

    </class>
</hibernate-mapping>